{
  "name": "protractor_test",
  "version": "1.0.0",
  "description": "Protractor using Cucumber Test Framework",
  "main": "protractor.config.js",
  "scripts": {
    "/": "Setup Preparation",
    "delete-temp-files":"node delete-temp-files.js",
    "tsc": "npm run delete-temp-files && tsc",
    "protractor": "./node_modules/protractor/build/cli.js",
    "webdriver-update-latest": "./node_modules/.bin/webdriver-manager update",
    "webdriver-update-specific": "./node_modules/.bin/webdriver-manager update --versions.chrome=78.0.3904.87",
    "report": "node -e  require('./temp/cucumber-html-reporter').ReportGenerator.generateReport();",
    "//": "Setup Builds",
    "test-jasmine": "npm run tsc && protractor ./temp/jasmine.config.js",
    "test-cucumber": "npm run tsc && protractor ./temp/cucumber.config.js",
    "e2e": "npm run test-cucumber",
    "e2e-report": "npm run test-cucumber & npm run report",
    "///": "To Debug: Open Chrome URL: chrome://inspect/#devices",
    "debug": "npm run tsc && node --inspect-brk ./node_modules/protractor/bin/protractor ./temp/cucumber.config.js"
  },
  "keywords": [
    "typescript protractor cucumber jasmine"
  ],
  "author": "Kenneth Mercado",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "^4.2.9",
    "@types/cucumber": "^6.0.1",
    "@types/jasmine": "3.5.4",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "^13.7.4",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "cucumber": "^6.0.5",
    "cucumber-html-reporter": "^5.1.0",
    "jasmine": "3.5.0",
    "protractor": "^5.4.3",
    "protractor-cucumber-framework": "^6.2.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  }
}